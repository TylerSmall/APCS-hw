public class Arrays {

    public int[] frontPiece(int[] nums) {
	int len = Math.min(2, nums.length);
	int[] newNums = new int[len];
	for (int i = 0; i < len; i++) {
	    newNums[i] = nums[i];
	}
	return newNums;
    }

    public int sum13(int[] nums) {
	int sum = 0;
	boolean last13 = false
	for (int i = 0; i < nums.length; i++) {
	    if (last13 == true && nums[i] != 1`) {
		
	}
	return sum;

    }
    

}
	
	
